{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ABHISHEKDF1"
		},
		"LS_BLOB_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_01'"
		},
		"LS_GIT_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_GIT_01'"
		},
		"LS_GIT_02_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_GIT_02'"
		},
		"LS_TABLE_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_TABLE_01'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ABHISHEKDF1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BLOB_01_connectionString": {
						"value": "[parameters('LS_BLOB_01_connectionString')]"
					},
					"LS_GIT_01_connectionString": {
						"value": "[parameters('LS_GIT_01_connectionString')]"
					},
					"LS_GIT_02_connectionString": {
						"value": "[parameters('LS_GIT_02_connectionString')]"
					},
					"LS_TABLE_01_connectionString": {
						"value": "[parameters('LS_TABLE_01_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}